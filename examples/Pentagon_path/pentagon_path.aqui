// Define a closed pentagon shape
shape path myPentagon {
  points: [
    [0, 50],   // Top vertex
    [47, 15],  // Top-right
    [29, -40], // Bottom-right
    [-29, -40],// Bottom-left
    [-47, 15], // Top-left
    [0, 50]    // Closing the shape (same as first point)
  ]
  position: [50, 50]
}

// Add the shape to a layer
layer main {
  add myPentagon
}
